{
  "compilerOptions": {
    "declaration": true,
    "baseUrl": ".",
    "module": "esnext",
    "target": "ES5",
    "sourceMap": true,
    "jsx": "react",
    "moduleResolution": "Node",
    "rootDir": ".",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": true
  },
  "include": [
  ],
  "exclude": [
    "node_modules",
    "build",
    "dist",
    "scripts",
    "acceptance-tests",
    "webpack",
    "jest",
    "src/setupTests.ts",
    "*.js"
  ]
}
